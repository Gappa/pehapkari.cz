{
    "name": "pehapkari/website",
    "description": "Website of Pehapkari.cz - Czech PHP Community",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "alterphp/easyadmin-extension-bundle": "^2.0",
        "chumper/zipper": "^1.0",
        "dfridrich/ares": "^1.2",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/orm": "2.7.x-dev",
        "easycorp/easyadmin-bundle": "^2.0",
        "fakturoid/fakturoid-php": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "k0nias/fakturoid-api": "dev-master",
        "knplabs/doctrine-behaviors": "^1.6",
        "sensio/framework-extra-bundle": "^5.2",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/event-dispatcher": "^4.2",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/orm-pack": "^1.0",
        "symfony/security": "^4.2",
        "symfony/security-bundle": "^4.2",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/templating": "^4.2",
        "symfony/twig-bridge": "^4.2",
        "symfony/twig-bundle": "^4.2",
        "symfony/yaml": "^4.2",
        "symplify/autodiscovery": "^5.4",
        "symplify/flex-loader": "^5.4",
        "symplify/statie": "^5.4",
        "twig/extensions": "^1.5",
        "vich/uploader-bundle": "^1.8"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-doctrine": "^0.11",
        "phpunit/phpunit": "^8.0",
        "symfony/browser-kit": "^4.2",
        "symfony/debug-bundle": "^4.2",
        "symfony/maker-bundle": "^1.11",
        "symfony/validator": "4.2.*",
        "symfony/web-profiler-bundle": "^4.2",
        "symfony/web-server-bundle": "^4.2",
        "symplify/coding-standard": "^5.4",
        "symplify/easy-coding-standard": "^5.4",
        "symplify/phpstan-extensions": "^5.4",
        "doctrine/annotations": "^1.6",
        "doctrine/cache": "^1.8",
        "latte/latte": "^2.5",
        "nette/application": "^2.4",
        "nette/forms": "^2.4",
        "nette/security": "^2.4",
        "symfony/console": "^4.2",
        "symfony/event-dispatcher": "^4.2",
        "symfony/expression-language": "^4.2",
        "symfony/intl": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "OpenTraining\\": "src",
            "OpenTraining\\Provision\\": "packages/Provision/src",
            "OpenTraining\\Registration\\": "packages/Registration/src",
            "OpenTraining\\KnowHow\\": "packages/KnowHow/src",
            "OpenTraining\\Training\\": "packages/Training/src",
            "OpenTraining\\Marketing\\": "packages/Marketing/src",
            "OpenTraining\\BetterEasyAdmin\\": "packages/BetterEasyAdmin/src",
            "OpenTraining\\User\\": "packages/User/src",
            "OpenTraining\\Statie\\": "statie/src"
        }
    },
    "autoload-dev":{
        "psr-4": {
            "OpenTraining\\Statie\\Tests\\": "statie/tests"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "check-cs": "vendor/bin/ecs check packages src statie",
        "fix-cs": [
            "vendor/bin/ecs check packages src statie/src statie/tests --fix",
            "bin/clear_trailing_whitespaces.sh"
        ],
        "doctrine": "bin/console doctrine:schema:update --dump-sql --force",
        "phpstan": "vendor/bin/phpstan analyse packages src statie/src statie/tests --error-format symplify"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.2.*"
        }
    }
}
