{
    "name": "open-project/auto-discovery",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "nette/utils": "^2.5",
        "symfony/dependency-injection": "^3.4|^4.1",
        "symfony/finder": "^3.4|^4.1"
    },
    "require-dev": {
        "symfony/framework-bundle": "^3.4|^4.1",
        "symfony/http-kernel": "^3.4|^4.1",
        "symfony/twig-bundle": "^3.4|^4.1",
        "doctrine/doctrine-bundle": "^1.9",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "OpenProject\\AutoDiscovery\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenProject\\AutoDiscovery\\Tests\\": "tests"
        },
        "classmap": ["tests/KernelProjectDir"]
    }
}
